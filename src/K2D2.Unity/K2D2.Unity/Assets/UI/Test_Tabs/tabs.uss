#tabs {
    flex-direction: row;
    -unity-font-style: bold;
    border-left-color: rgb(76, 79, 188);
    border-right-color: rgb(76, 79, 188);
    border-top-color: rgb(76, 79, 188);
    border-bottom-color: rgb(76, 79, 188);
    border-bottom-width: 2px;
}

.unselectedContent {
    display: none;
}

.unity-button.tab.currentlySelectedTab {
    background-color: rgba(0, 255, 51, 0.39);
}

.running_a {
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0);
    right: 10px;
    display: flex;
    position: absolute;
    top: 10px;
    width: 40px;
    height: 40px;
    transition-timing-function: ease-in-sine;
    transition-duration: 1s;
}

.is_running_light {
    flex-grow: 1;
    background-color: rgb(60, 58, 67);
    display: flex;
    opacity: 1;
    height: 10px;
    position: absolute;
    top: 0;
    right: 0;
    border-radius: 7px;
    border-width: 1px;
    border-color: rgb(91, 91, 91);
}

.is_running_on {
    background-color: rgba(0, 255, 116, 0.86);
    transition-duration: 1s;
    transition-timing-function: ease-in-out-sine;
}

.tab {
    width: auto;
    height: 25px;
    padding-bottom: 0;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    overflow: hidden;
    margin-left: 0;
    margin-right: 2px;
    flex-grow: 0;
    background-color: rgb(22, 22, 22);
    border-color: rgb(51, 51, 51);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    padding-top: 0;
    padding-left: 4px;
    padding-right: 4px;
    flex-direction: column;
    justify-content: flex-end;
}

.tab.current {
    height: 28px;
    align-self: auto;
    flex-direction: column;
    justify-content: flex-end;
    border-left-color: rgb(51, 51, 51);
    border-right-color: rgb(51, 51, 51);
    border-top-color: rgb(51, 51, 51);
    border-bottom-color: rgb(51, 51, 51);
    padding-bottom: 2px;
}

.tab > Label {
}

.tab:hover {
    background-color: rgb(40, 40, 41);
}

.tab:active {
    background-color: rgb(57, 0, 255);
}
